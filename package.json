{
  "name": "stock-app",
  "version": "1.0.0",
  "description": "a stock market app from the fcc fullstack challenges",
  "main": "server.js",
  "scripts": {
    "start": "node build/server/backend.js",
    "start:dev": "npm run build:dev && node build/server/backend.js",
    "start:prod": "set NODE_ENV=production&& npm run build:prod && node build/server/backend.js",
    "dev-server": "webpack-dev-server",
    "build:dev": "npm run build:server:dev && npm run build:client:dev",
    "build:prod": "npm run build:server && npm run build:client",
    "build:client": "webpack",
    "build:client:dev": "webpack",
    "build:server": "webpack",
    "build:server:dev": "webpack",
    "postinstall": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkoN95/Stock-App.git"
  },
  "keywords": [
    "webSockets",
    "stock-market",
    "react",
    "fcc"
  ],
  "author": "MarkoN95",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkoN95/Stock-App/issues"
  },
  "homepage": "https://github.com/MarkoN95/Stock-App#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "connect-logger": "0.0.1",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "^2.28.0",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1",
    "webpack-validator": "^2.3.0"
  },
  "dependencies": {
    "async": "^2.1.4",
    "highcharts": "^8.2.0",
    "pug": "^2.0.0-beta11",
    "quandl": "0.0.6",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "redis": "^2.6.5",
    "socket.io": "^1.7.2"
  }
}
